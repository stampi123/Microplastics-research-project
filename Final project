    "#Renaming columns to merge\n",
    "PP_YearlyCount_worldwide = PP_YearlyCount_worldwide.rename(columns={'Year Agreed': 'Year'})\n",
    "df_final = df_final.reset_index().rename(columns={'Date': 'Year'})\n",
