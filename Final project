    "# Calculate the best-fit line for the non-standardized density class\n",
    "coef_non_standardized = np.polyfit(merged_df['# of Plastic Policies passed'], merged_df['Density Class'], 1)\n",
    "slope_non_standardized = coef_non_standardized[0]\n",
    "\n",
    "# Calculate the best-fit line for the standardized density class\n",
    "coef_standardized = np.polyfit(merged_df['# of Plastic Policies passed'], merged_df['Scaled Density'], 1)\n",
    "slope_standardized = coef_standardized[0]\n",
    "\n",
    "# Print the slopes for comparison\n",
    "print(f\"Slope of the best-fit line (non-standardized): {slope_non_standardized}\")\n",
    "print(f\"Slope of the best-fit line (scaled density): {slope_standardized}\")"
   ]
