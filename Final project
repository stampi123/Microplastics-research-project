   "source": [
    "# create new columns Density Rank (s) that integer ranks all average concentration densities\n",
    "merged_df['Density Rank'] = merged_df['Density Class'].rank()\n",
    "\n",
    "# Take a look!\n",
    "merged_df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 101,
   "metadata": {},
   "outputs": [],
   "source": [
    "# define function to run Mann Whitney U test \n",
    "# parameters idx, basis, and alternative \n",
    "# returns p value\n",
    "def run_MWUtest (idx, alt = \"two-sided\"):\n",
    "    prior = merged_df['Density Rank'].iloc[:idx + 1]\n",
    "    post = merged_df['Density Rank'].iloc[idx + 1:]\n",
    "    stat,p = mannwhitneyu(prior,post,alternative=alt, method = \"exact\")\n",
    "    return p"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 120,
   "metadata": {},
   "outputs": [],
   "source": [
    "# create 3 new empty columns in dataframe to accomodate test\n",
    "merged_df['Density Class 2-s']= np.nan\n",
    "merged_df['Density Class 1-g']= np.nan\n",
    "merged_df['Density Class 1-l']= np.nan\n",
    "merged_df['S G L Results'] = [(np.nan,np.nan,np.nan)] * len(merged_df)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 136,
   "metadata": {},
   "outputs": [],
   "source": [
    "%%capture\n",
    "# silencing some dataframe assignment warnings\n",
    "\n",
    "# apply function to calculate p values for all years 1992-2017\n",
    "for i in range(2,29):\n",
    "    # run 2-sided Mann Whitney U test (hypothesizing that the distributions are not equal)\n",
    "    merged_df.loc[i,'Density Class 2-s'] = run_MWUtest(i)\n",
    "    # run 1-sided Mann Whitney U test (hypothesizing that the prior distribution is greater than post)\n",
    "    merged_df.loc[i,'Density Class 1-g'] = run_MWUtest(i,alt=\"greater\")\n",
    "    # run 1-sided Mann Whitney U test (hypothesizing that the prior distribution is less than post)\n",
    "    merged_df.loc[i,'Density Class 1-l'] = run_MWUtest(i,alt=\"less\")\n",
    "\n",
    "    # using an alpha of 0.05, we evaluate via the p values if each test does not reject (true)\n",
    "    # or rejects (false) the null hypothesis in favor of our alternative\n",
    "    # We store the results in a tuple of 3 boolean factors\n",
    "    s = bool(merged_df.loc[i,'Density Class 2-s'] > 0.05)\n",
    "    g = bool(merged_df.loc[i,'Density Class 1-g'] > 0.05)\n",
    "    l = bool(merged_df.loc[i,'Density Class 1-l'] > 0.05)\n",
    "    merged_df['S G L Results'][i] = [(s,g,l)]\n",
    "    "
   ]
  },
