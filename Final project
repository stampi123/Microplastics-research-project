   "source": [
    "# In order to analyze the data in terms of years, we define a function that\n",
    "# converts all of the 'Date' column into datetime objects to extract year\n",
    "\n",
    "def standardize_date(d):\n",
    "    if ',' in d:\n",
    "        d = (d.split(','))[0]\n",
    "        \n",
    "    d = (d.split())[0]\n",
    "    d_dt = datetime.strptime(d,'%m/%d/%Y')\n",
    "\n",
    "    return d_dt.year"
   ]
  },
