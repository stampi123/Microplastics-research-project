    "#Import population data frame\n",
    "populationdf = pd.read_csv('https://raw.githubusercontent.com/exmills/Cogs108-CSV-Files/main/worldpopulation.csv')\n",
    "\n",
    "#We only care about the global population\n",
    "#Filter for 'World' in Entity\n",
    "populationdf = populationdf[populationdf['Entity'] == 'World']\n",
    "\n",
    "\n",
    "#We want the years 1972 - 2023\n",
    "populationdf = populationdf[(populationdf['Year'] >= 1972) & (populationdf['Year'] <= 2023)]\n",
    "\n",
    "#Let's look at population over time\n",
    "populationdf = populationdf[['Year', 'Population (historical estimates)']]\n",
    "\n",
    "# Look at the head of the populationdf\n",
    "populationdf.reset_index(drop=True, inplace=True)\n",
    "print(populationdf.head())\n",
    "\n",
    "# Plot the data\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.plot(populationdf['Year'], populationdf['Population (historical estimates)'])\n",
    "plt.title('World Population from 1972 to 2023')\n",
    "plt.xlabel('Year')\n",
    "plt.ylabel('Population (historical estimates)')\n",
    "plt.show()"
   ]
